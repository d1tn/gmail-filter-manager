{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "version": "1.4.0",
  "description": "__MSG_extensionDescription__",
  "permissions": [
    "storage"
  ],
  "default_locale":"en",
  "web_accessible_resources": [
    {
      "resources": [
        "docs/*",
        "lib/*",
        "updates/*",
        "manager.html",
        "manager.js",
        "manager.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "host_permissions": [
    "https://mail.google.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "ui.css"
      ]
    }
  ],
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Gmail Filter Manager"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "author": "d1tn",
  "homepage_url": "https://github.com/d1tn/gmail-filter-manager"
}